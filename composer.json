{
    "name": "monomelodies/dormant",
    "description": "Dabble-based Ornament adapter",
    "license": "MIT",
    "keywords": ["php","orm"],
    "authors": [
        {
            "name": "Marijn Ophorst",
            "email": "marijn@monomelodies.nl"
        }
    ],
    "require": {
        "php": ">=5.4",
        "monomelodies/dabble": "^1.5.2",
        "monomelodies/ornament": "^0.8.3"
